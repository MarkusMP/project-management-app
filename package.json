{
  "scripts": {
    "up:docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "down:docker:dev": "docker-compose -f docker-compose.dev.yml down -v",
    "build:docker:dev": "docker-compose -f docker-compose.dev.yml build",
    "up:docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "down:docker:prod": "docker-compose -f docker-compose.prod.yml down -v",
    "build:docker:prod": "docker-compose -f docker-compose.prod.yml build"
  }
}
